    "require": {
        "php": ">=5.3.0",
        "symfony/console": "~2.6|~3.0",
        "symfony/process": "~2.6|~3.0",
        "symfony/config": "~2.6|~3.0",
        "cocur/slugify": "~1.0",
        "symfony/yaml": "~2.6|~3.0"
    },
    "require-dev": {
        "phpunit/phpunit": "*"
    },
    "bin": [
        "bin/migrate"
    ],
    "config": {
      "bin-dir": "bin"
    },
    "autoload": {
      "psr-4": {"Migrate\\": "src/Migrate/"}
